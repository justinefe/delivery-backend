{
  "name": "delivery",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/server.js",
    "dev": "nodemon --exec babel-node server/server.js",
    "migrate_test": "cross-env NODE_ENV=test sequelize db:migrate",
    "migrate_test:undo": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "pretest": "npm run migrate_test:undo && npm run migrate_test",
    "migrate": "cross-env sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo",
    "build": "babel server --copy-files --out-dir distr && cross-env NODE_ENV=production sequelize db:migrate && ls -a",
    "build_dev": "babel server --copy-files --out-dir dist && cross-env NODE_ENV=production sequelize db:migrate"
  },
